<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.devapps.yams.projectgutenbergauthorbroswer" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/gutenberg"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.devapps.yams.projectgutenbergauthorbroswer.ProjectGmainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.devapps.yams.projectgutenbergauthorbroswer.AuthorActivity"></activity>



    </application>
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
</manifest>
